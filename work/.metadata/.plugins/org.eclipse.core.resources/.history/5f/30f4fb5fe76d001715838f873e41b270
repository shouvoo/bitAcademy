package basic;

public class Calculate 
{

	public static void main(String[] args) 
	{
		// 5 + 3 = 8; 주석 ::: 코딩의 설명이나 부가적 내용을 기술시 사용
		/*
			8 = 5 + 3;
			주석구간
		*/
		int x;
		x = 10;
		int y = 20, z, a = 10 + 20;
		//콤마를 사용 변수를 동시에 선언할수있다
		
		int b = y + a;
		
		System.out.println("x  :  " + x);
		System.out.println("y  :  " + y);
		System.out.println("a  :  " + a);
		System.out.println("b  :  " + b);
	}

}
