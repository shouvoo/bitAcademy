package basic;

public class Forfor 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		for (int h = 0; h <= 12; h++) 
		{
			//System.out.println(h + "     ½Ã>>>>>>>");
			for (int m = 0; m < 60; m++) {
				//System.out.println(h + "  :  " + m);
			}
		}
		
		for (int i = 1; i < 10; i++) 
		{
			for (int j = 1; j < 10; j++) 
			{
				//System.out.println(i + "  *  " + j + "  =  " + (i * j));
			}
		}
		
		int oldI = 1;
		for (int i = 1; i < 10; i++) 
		{
			
			String enter = "";
			
			if(oldI != i) enter = "\n";
			else enter = "";
			
			System.out.println(oldI);
			
			for (int j = 1; j < 10; j++) 
			{
				//System.out.print(enter + (j + " * " + i) + " = " + (j * i) + "\t");
			}
			
			oldI = i;
		}
	}

}
