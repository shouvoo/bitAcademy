package basic;

public class SwitchMain2 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		
		int avg = 97;
		
		switch(avg / 10)
		{
			case 10 :
			case 9 :
				System.out.println("수");
				break;
			case 8 :
				System.out.println("우");
				break;
			case 7 :
				System.out.println("미");
				break;
			case 6 :
				System.out.println("양");
				break;
			default :
				System.out.println("가");
				break;
		}
		
		double avg2 = 3.8;
		String str;
		switch((int)(avg2 * 2))
		{
			case 9 :
				str = "";
				break;
			case 8 :
				str = "";
				break;
			case 7 :
				str = "";
				break;
			case 6 :
				str = "";
				break;
			case 5 :
				str = "";
				break;
			case 4 :
				str = "";
				break;
			case 3 :
				str = "";
				break;
			default :
				System.out.println("D");
				break;
		}
	}

}




































































