package basic;

public class SwitchMain2 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		
		int avg = 88;
		
		
		switch(avg / 10)
		{
			case 10 :
				System.out.println("快");
				break;
			case 9 :
				System.out.println("快");
				break;
			case 8 :
				System.out.println("快");
				break;
			case 7 :
				System.out.println("快");
				break;
			case 6 :
				System.out.println("快");
				break;
			default :
				System.out.println("快");
				break;
		}

	}

}




































































