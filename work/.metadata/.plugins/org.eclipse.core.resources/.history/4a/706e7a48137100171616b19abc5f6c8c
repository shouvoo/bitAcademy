package basic;

public class SwitchMain 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		switch(1)
		{
			case 1 :
				System.out.println("one");
				break;
			case 2 : 
				System.out.println("two");
				break;
			case 3 : 
				System.out.println("three");
				break;
			default :
				System.out.println("default");
				break;
		}

	}

}
