package basic;

public class IfMain 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int a = 87;
		String sss = "기본값";
		if(a > 90)
		{
			System.out.println("난 참이야");
			sss = "참참참";
		} 
		if(a > 80) {
			sss = "난 80이야";
		} else {
			sss = "뻥뻥뻥";
		}
		System.out.println(sss);
	}

}
