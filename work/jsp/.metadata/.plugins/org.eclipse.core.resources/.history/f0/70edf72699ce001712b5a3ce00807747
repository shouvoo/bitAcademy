package com.omp.repository.mapper;

import java.util.List;

import com.omp.repository.domain.MembershipDomain;

public interface MembershipMapper 
{
	public void insertMemberComment(MembershipDomain membership) throws Exception;
	public void deleteMemberComment(MembershipDomain membership) throws Exception;
	public void modifyMemberComment(MembershipDomain membership) throws Exception;
	public List<MembershipDomain> readMemberComment(MembershipDomain membership) throws Exception;
	public MembershipDomain detailMemberComment(MembershipDomain membership) throws Exception;
	public int totalMembershipComment() throws Exception;
}
