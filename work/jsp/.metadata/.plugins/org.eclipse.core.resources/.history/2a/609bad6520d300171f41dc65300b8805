<!-- 
	animate(properties[, duration][, easing][, complete]) : 애니메이션 등록하기
 -->
<!DOCTYPE html>
<html>
<head>
    <style>
        div {
            width: 50px; height: 50px;
            background-color: orange;
			position: relative;
        }
    </style>
    <script src="../js/jquery-3.2.1.js"></script>
</head>
<body>
    <div></div><div></div>
    <div></div><div></div>
    <div></div><div></div>
    <script>
		/* $("div").click(function(){
			$(this).animate({left: 300}, "slow");
		}); */
		
		$("div").hover(
				function(){
					//마우스가 over일때
					$(this).animate({margin: 50}, "slow");
				},
				
				function(){
					//마우스가 out일때
					$(this).animate({margin: 0}, "slow");
				}
		);
		
    </script>
</body>
</html>












