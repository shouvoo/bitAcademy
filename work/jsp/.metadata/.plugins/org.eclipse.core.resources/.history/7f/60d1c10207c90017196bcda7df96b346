<!-- 
	getAttribute() 기능 추가하기
	
	el.getAttribute("width");
	
	추출 : attr("속성명");
	
	예> attr("width");

 -->


<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript" src="jquery.js"></script>
</head>
<body>
	<button>확인</button>
	<div id="result" data-count="1">속성확인용</div>
	
	<script type="text/javascript">
	
		var btn = $("button")[0];
		btn.addEventListener("click", function(e) {
			console.log($("#result").attr("id"));	
			console.log($("#result").attr("data-count"));	
		})
	
	</script>
</body>
</html>