	<button>요청</button>
	<div id="result"></div>
	<script>
	$("button").click(function(){
		console.log("location.origin : ", location.origin)
		$.ajax({
			url: "http://localhost:9080/lec06_apiserver/crossdomain/test01"
		})
		.done(function(data){
			$("#result").html(data);
		});
	})
	</script>
